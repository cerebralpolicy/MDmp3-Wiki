@import "./custom/custom";

.ArtistSectionHead{
    display: flex;
    align-items: center;
    width: 100%;
}

.ArtistSectionHead .FlagContainer{
    width:64px; 
    height:32px; 
    background: black;
    clip-path: polygon(12% 16%, 100% 16%, 88% 84%, 0 84%);
}

.ArtistSectionHead .FlagContainer .FlagViewBox{
    width:100%; height:100%; 
    clip-path: polygon(14% 20%, 98% 20%, 88% 80%, 2% 80%); 
    text-align:center; 
    background: white;
}